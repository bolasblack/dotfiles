[user]
	name = c4605
	email = bolasblack@gmail.com
[core]
	editor = vim
	excludesfile = ~/dotfiles/.gitignore_global
[color]
	status = auto
	branch = auto
	diff = auto
	ui = true
	pager = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	co = checkout
	cb = checkout -b
	br = branch
	cm = commit
	st = status
[push]
	default = upstream
[github]
	user = bolasblack
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[diff]
	noprefix = true
